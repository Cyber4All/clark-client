{
  "name": "clark",
  "displayName": "CLARK: Cybersecurity Labs and Resource Knowledge-base",
  "version": "6.11.4",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --configuration=production",
    "prebuild": "echo \"export const COMMIT_HASH = '$(git rev-parse --short HEAD)';\" > src/commit-hash.ts",
    "build": "npm run prebuild && ng build clark",
    "build:staging": "npm run prebuild && ng build clark --configuration=staging",
    "build:prod": "npm run prebuild && ng build clark --configuration=production && npm run rum:upload-prod",
    "rum:install": "npm list -g @coralogix/rum-cli || npm install -g @coralogix/rum-cli",
    "rum:upload-staging": "npm run rum:install && coralogix-rum-cli upload-source-maps -k \"$CORALOGIX_SOURCE_MAP_KEY\" -a \"clark-staging\" -e \"US1\" -v \"${RUM_VERSION:-$npm_package_version}\" -f \"./dist/\" || echo \"RUM upload failed, continuing...\"",
    "rum:upload-prod": "npm run rum:install && coralogix-rum-cli upload-source-maps -k \"$CORALOGIX_SOURCE_MAP_KEY\" -a \"clark-production\" -e \"US1\" -v \"${RUM_VERSION:-$npm_package_version}\" -f \"./dist/\" || echo \"RUM upload failed, continuing...\"",
    "test:unit": "jest --maxWorkers=2 --verbose --coverage uri-retriever featuredObjects",
    "lint": "npx ng lint clark",
    "lint:fix": "npx ng lint clark --fix",
    "prettier": "prettier --check .",
    "e2e": "ng e2e",
    "storybook": "start-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.14",
    "@angular/cli": "^16.2.16",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@babel/core": "^7.16.7",
    "@coralogix/browser": "^2.9.0",
    "@cyber4all/clark-taxonomy": "^4.0.5",
    "@types/node": "^22.15.17",
    "aws-sdk": "^2.1058.0",
    "core-js": "^3.6.5",
    "md5": "^2.2.1",
    "ng-click-outside": "^9.0.1",
    "ng2-charts": "^4.1.1",
    "ngx-cookie": "^5.0.2",
    "ngx-cookie-service": "^16.1.0",
    "ngx-simple-text-editor": "^4.0.3",
    "ngx-virtual-scroller": "^4.0.3",
    "raven-js": "^3.22.3",
    "request-promise": "^4.2.5",
    "rxjs": "^6.5.4",
    "title-case": "^3.0.3",
    "tslib": "^2.3.1",
    "util": "^0.12.4",
    "uuid": "7.0.3",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular-eslint/builder": "^13.5.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "^19.3.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/language-service": "^16.2.12",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "ajv": "^6.12.6",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^29.7.0",
    "babel-loader": "^8.0.6",
    "canvas": "^2.11.2",
    "chart.js": "^4.4.9",
    "chrome-launcher": "^0.13.4",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "^37.6.1",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "^8.0.1",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.6",
    "prettier": "^3.1.1",
    "protractor": "~7.0.0",
    "sass": "^1.46.0",
    "ts-jest": "^29.3.4",
    "tslib": "^1.10.0",
    "typescript": "^4.9.3"
  },
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": ">=v16.13.2 <17",
    "npm": ">=8"
  }
}
