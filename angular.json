{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "clark": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist"
            },
            "index": "src/index.html",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": [
              "@angular/localize/init",
              "src/polyfills.ts"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/env.js"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "./node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "src/globals.scss",
              "src/mat-input.scss"
            ],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": [
              "@cyber4all/clark-taxonomy",
              "aws-sdk",
              "chart.js",
              "md5",
              "ng-click-outside",
              "rxjs/internal/observable/of"
            ],
            "browser": "src/main.ts"
          },
          "configurations": {
            "experimental": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment-experimental.ts"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "clark:build"
          },
          "configurations": {
            "experimental": {
              "buildTarget": "clark:build:experimental"
            },
            "production": {
              "buildTarget": "clark:build:prod"
            },
            "staging": {
              "buildTarget": "clark:build:staging"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "clark:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": [
              "@angular/localize/init",
              "src/polyfills.ts"
            ],
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "src/globals.scss"
            ],
            "assets": ["src/assets", "src/favicon.ico", "src/env.js"]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        }
      }
    },
    "storybook": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": [
              "@angular/localize/init",
              "src/polyfills.ts"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/env.js"
            ],
            "styles": [
              "src/globals.scss"
            ],
            "scripts": [],
            "outputPath": {
              "base": "dist"
            },
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "defaultConfiguration": ""
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "clark",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "clark"
    }
  },
  "cli": {
    "analytics": false
  }
}
